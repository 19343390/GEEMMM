
exports.Chot_display = function(Chot_class, centre){ 
  Chot_class = ee.Image(Chot_class);
  var tf = Chot_class.select('classification').eq(1).selfMask();
  var cc = Chot_class.select('classification').eq(11).selfMask();
  var co = Chot_class.select('classification').eq(15).selfMask();
  var eb = Chot_class.select('classification').eq(19).selfMask();
  var vo = Chot_class.select('classification').eq(22).selfMask();
  var ub = Chot_class.select('classification').eq(38).selfMask();
  var rw = Chot_class.select('classification').eq(43).selfMask();
  var nf = Chot_class.select('classification').eq(70).selfMask();            
            
 //Adding three mangrove classes - some issues in this code leading to masking errors
            
 centre.addLayer(tf, {palette: ['#E69800']}, 'Cont. HOT Terrestrial Forest',false);
 centre.addLayer(cc, {palette: ['#267300']}, 'Cont. HOT Closed-Canopy Mangrove',false);
 centre.addLayer(co, {palette: ['#4CE600']}, 'Cont. HOT Open-Canopy Mangrove I',false);
 centre.addLayer(eb, {palette: ['#8400A8']}, 'Cont. HOT Exposed / Barren',false);
 centre.addLayer(vo, {palette: ['#CDF57A']}, 'Cont. HOT Open-Canopy Mangrove II',false);
 centre.addLayer(ub, { palette: ['#00FFC5']}, 'Cont. HOT Urban / Built',false);
 centre.addLayer(rw, {palette: ['#004DA8']}, 'Cont. HOT Residual Water',false);
 centre.addLayer(nf, { palette: ['#FFFF00']}, 'Cont. HOT Non-Forest Vegetation',false);
};           

